{
  "name": "@robpc/git-semver",
  "version": "0.1.7",
  "description": "Generates a valid semver of any commit based on the distance to the last semver tag",
  "repository": "github:robpc/git-semver",
  "targets": {
    "main": {
      "source": "src/index.ts",
      "context": "node"
    },
    "module": {
      "source": "src/index.ts",
      "outputFormat": "esmodule",
      "context": "node"
    },
    "cli": {
      "source": "src/cli.ts",
      "context": "node"
    },
    "types": {
      "source": "src/index.ts"
    }
  },
  "source": "src/index.ts",
  "main": "dist/main.js",
  "bin": "dist/cli.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "start": "node dist/cli.js",
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "jest",
    "example:alpha": "env-cmd npm run start robpc/git-version-tests-alpha 88f8ebe",
    "example:alpha2": "env-cmd npm run start robpc/git-version-tests-alpha a7b54fe",
    "example:alpha3": "env-cmd npm run start robpc/git-version-tests-alpha ddf0a84"
  },
  "author": "Rob Cannon",
  "license": "MIT",
  "keywords": [
    "git",
    "version",
    "semver"
  ],
  "devDependencies": {
    "@parcel/packager-ts": "^2.2.0",
    "@parcel/transformer-typescript-types": "^2.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.6",
    "@types/semver": "^7.3.9",
    "env-cmd": "^10.1.0",
    "jest": "^27.4.7",
    "parcel": "^2.2.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@octokit/rest": "^18.12.0",
    "@robpc/logger": "^1.1.0",
    "semver": "^7.3.5"
  }
}
